#include<iostream>
using namespace std;
int main(){
    string s;
    s="kanak";
    string s2;
    for(int i=s.length();i>0;i--){
        s2+=s[i-1];
    }
    bool flag=true;
    for (int i = 0; i < s.length(); i++){
        
        if(s[i]!=s2[i]){
            cout<<"Not a palindrome"<<endl;
            flag=false;
            return 0;
        }
    }
    if(flag){
            cout<<" its a Palindrome"<<endl;
        }
    cout<<s2<<endl;
    return 0;
}
